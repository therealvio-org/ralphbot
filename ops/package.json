{
  "name": "ralphbot",
  "version": "0.1.0",
  "bin": {
    "ralphbot": "bin/ralphbot.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "clean": "yarn build --build --clean",
    "eslint": "eslint . --ext .ts --ignore-pattern '*.d.ts'",
    "prettier": "prettier --check '**/*'",
    "lint": "yarn -s eslint && yarn -s prettier",
    "lint:fix": "tsc && yarn -s eslint --fix && yarn -s prettier --write",
    "deploy": "cdk deploy '*' --require-approval never",
    "ci-deploy": "cdk deploy '*' --require-approval never --progress events"
  },
  "devDependencies": {
    "@types/jest": "29.5.1",
    "@types/node": "18.15.11",
    "@typescript-eslint/eslint-plugin": "5.59.0",
    "@typescript-eslint/parser": "5.59.0",
    "aws-cdk": "2.75.0",
    "eslint": "8.38.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-only-warn": "1.1.0",
    "eslint-plugin-sort-imports-es6-autofix": "0.6.0",
    "jest": "29.5.0",
    "prettier": "2.8.7",
    "ts-jest": "29.1.0",
    "node": "18.15.0",
    "ts-node": "10.9.1",
    "typescript": "5.0.4"
  },
  "dependencies": {
    "aws-cdk-lib": "2.75.0",
    "constructs": "10.2.0",
    "source-map-support": "0.5.21"
  }
}
