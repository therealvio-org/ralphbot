{
  "name": "ralphbot",
  "version": "0.1.0",
  "bin": {
    "ralphbot": "bin/ralphbot.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "clean": "yarn build --build --clean",
    "eslint": "eslint . --ext .ts --ignore-pattern '*.d.ts'",
    "prettier": "prettier --check '**/*'",
    "lint": "yarn -s eslint && yarn -s prettier",
    "lint:fix": "tsc && yarn -s eslint --fix && yarn -s prettier --write",
    "deploy": "cdk deploy '*' --require-approval never",
    "ci-deploy": "cdk deploy '*' --require-approval never --progress events"
  },
  "devDependencies": {
    "@types/jest": "29.2.0",
    "@types/node": "16.18.0",
    "@typescript-eslint/eslint-plugin": "5.41.0",
    "@typescript-eslint/parser": "5.41.0",
    "aws-cdk": "2.47.0",
    "eslint": "8.26.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-only-warn": "1.0.3",
    "eslint-plugin-sort-imports-es6-autofix": "0.6.0",
    "jest": "29.2.2",
    "prettier": "2.7.1",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "typescript": "4.8.4"
  },
  "dependencies": {
    "aws-cdk-lib": "2.47.0",
    "constructs": "10.1.139",
    "source-map-support": "0.5.21"
  }
}
