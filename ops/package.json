{
  "name": "ralphbot",
  "version": "0.1.0",
  "packageManager": "pnpm@8.8.0",
  "bin": {
    "ralphbot": "bin/ralphbot.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "clean": "pnpm build --build --clean",
    "eslint": "eslint . --ext .ts --ignore-pattern '*.d.ts'",
    "prettier": "prettier --check '**/*'",
    "lint": "pnpm -s eslint && pnpm -s prettier",
    "lint:fix": "tsc && pnpm -s eslint --fix && pnpm -s prettier --write",
    "deploy": "cdk deploy '*' --require-approval never",
    "ci-deploy": "cdk deploy '*' --require-approval never --progress events --concurrency 2"
  },
  "devDependencies": {
    "@types/jest": "29.5.5",
    "@types/node": "18.18.3",
    "@typescript-eslint/eslint-plugin": "6.7.4",
    "@typescript-eslint/parser": "6.7.4",
    "aws-cdk": "2.99.1",
    "eslint": "8.50.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-only-warn": "1.1.0",
    "eslint-plugin-sort-imports-es6-autofix": "0.6.0",
    "jest": "29.7.0",
    "prettier": "3.0.3",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "5.2.2"
  },
  "dependencies": {
    "aws-cdk-lib": "2.99.1",
    "constructs": "10.2.70",
    "source-map-support": "0.5.21"
  }
}
