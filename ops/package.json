{
  "name": "ralphbot",
  "version": "0.1.0",
  "packageManager": "pnpm@9.15.5",
  "bin": {
    "ralphbot": "bin/ralphbot.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "clean": "pnpm build --build --clean",
    "eslint": "eslint --ignore-pattern '*.d.ts' .",
    "prettier": "prettier --check '**/*'",
    "lint": "pnpm -s eslint && pnpm -s prettier",
    "lint:fix": "pnpm -s eslint --fix && pnpm -s prettier --write",
    "deploy": "cdk deploy '*' --require-approval never",
    "ci-deploy": "cdk deploy '*' --require-approval never --progress events --concurrency 2"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.6",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.20.0",
    "@types/jest": "29.5.14",
    "@types/node": "20.12.7",
    "@typescript-eslint/eslint-plugin": "8.24.0",
    "@typescript-eslint/parser": "8.24.0",
    "aws-cdk": "2.178.2",
    "eslint": "9.20.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-only-warn": "1.1.0",
    "eslint-plugin-sort-imports-es6-autofix": "0.6.0",
    "jest": "29.7.0",
    "prettier": "3.5.1",
    "ts-jest": "29.2.5",
    "ts-node": "10.9.2",
    "typescript": "5.7.3"
  },
  "dependencies": {
    "aws-cdk-lib": "2.178.2",
    "constructs": "10.4.2",
    "source-map-support": "0.5.21"
  }
}
